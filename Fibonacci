def fibonacci(num):
    if num == 0:
        return 0
    elif num == 1:
        return 1
    else:
        return fibonacci(num - 1) + fibonacci(num - 2)

n = int(input("Enter the number of Fibonacci numbers to print: "))
print(f"The first {n} Fibonacci numbers are:")
for i in range(n):
    print(fibonacci(i), end=" ")
